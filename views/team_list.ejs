<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>M LEAGUE DB</title>
    <link rel="stylesheet" href="/css/style.css">
  </head>
  <body>
    <header>
        <div class="container">
            <a href="/">M LEAGUE DB</a>
        </div>
    </header>
    <div class="container">
      <div class="container-header">
        <h1>チーム一覧</h1>
      </div>
      <div class="index-table-wrapper">
        <!-- <div class="table-head">
          <span>チーム名</span>
        </div> -->
        <ul class="table-body">
          <% teams.forEach((team) => { %>
            <li>
              <div class="team-data">
                <span class="name-column"><%= team.name %></span>
              </div>
            </li>
          <% }) %>
        </ul>
      </div>
      <a href="/" class="index-button">もどる</a>
    </div>
  </body>
</html>